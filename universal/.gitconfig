[user]
    email = m@mdorst.net
    user = git
    name = Michael Dorst
[alias]
    s = status -s -u
    l = log --oneline --graph --branches --remotes --tags HEAD
    ll = log --graph --branches --remotes --tags HEAD
	c = commit
    ls = ls-tree -r --name-only
    unstage = reset HEAD
	fixup = commit --fixup HEAD
	update = fetch --all --prune
	co = checkout
[core]
    pager = less -X -F
[color]
    ui = always
[credential]
    helper = store
[rebase]
	autosquash = true
